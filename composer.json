{
    "name": "bugloos/responder-bundle",
    "description": "Responder Bundle",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mojtaba Gheytasi",
            "email": "mojtaba.g@bugloos.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bugloos\\ResponderBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bugloos\\ResponderBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^4.4 | ^5.4 | ^6.0",
        "pagerfanta/pagerfanta": "^3.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^5.3",
        "doctrine/orm": "^2.7",
        "doctrine/doctrine-bundle": "^2.0.3"
    },
    "conflict": {
        "symfony/http-foundation": "<4.4",
        "symfony/framework-bundle": "<4.4",
        "doctrine/orm": "<2.7"
    }
}
